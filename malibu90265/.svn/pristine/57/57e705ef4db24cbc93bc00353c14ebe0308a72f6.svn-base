<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="teamController" class="com.sqisoft.iqs.team.controller.TeamController" >
		<property name="teamService" ref="teamService"/>
	</bean>
	
	<bean id="totalController" class="com.sqisoft.iqs.team.controller.TotalController">
		<property name="totalService" ref="totalService"/>
		<property name="teamService" ref="teamService"/>
	</bean>

	
</beans>
