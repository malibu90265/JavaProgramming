<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- 월별 투입 MAP -->
	
	<bean id="costMapDAO" class="com.sqisoft.iqs.humanResource.costmap.dao.impl.CostMapDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="costMapService" class="com.sqisoft.iqs.humanResource.costmap.service.impl.CostMapServiceImpl">
		<property name="costMapDAO" ref="costMapDAO"/>
		<property name="employeeInputDAO" ref="employeeInputDAO"/>
		<property name="projectDAO" ref="projectDAO"/>
		<property name="teamDAO" ref="teamDAO"/>
		<property name="waitingListDAO" ref="waitingListDAO"/>
	</bean>
	
</beans>
