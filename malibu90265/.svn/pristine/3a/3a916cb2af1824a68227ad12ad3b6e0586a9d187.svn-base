package com.sqisoft.iqs.project.vo;

public class ProjectStatusVO {

	private int pj_status_id;
	private String pj_status_name;
	private String pj_status_color;
	private String pj_status_text_color;

	public int getPj_status_id() {
		return pj_status_id;
	}

	public void setPj_status_id(int pj_status_id) {
		this.pj_status_id = pj_status_id;
	}

	public String getPj_status_name() {
		return pj_status_name;
	}

	public void setPj_status_name(String pj_status_name) {
		this.pj_status_name = pj_status_name;
	}

	public String getPj_status_color() {
		return pj_status_color;
	}

	public void setPj_status_color(String pj_status_color) {
		this.pj_status_color = pj_status_color;
	}

	public String getPj_status_text_color() {
		return pj_status_text_color;
	}

	public void setPj_status_text_color(String pj_status_text_color) {
		this.pj_status_text_color = pj_status_text_color;
	}
}
