<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	<bean id="waitingListDAO" class="com.sqisoft.iqs.humanResource.waitingList.dao.impl.WaitingListDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="waitingListService" class="com.sqisoft.iqs.humanResource.waitingList.service.impl.WaitingListServiceImpl">
		<property name="waitingListDAO" ref="waitingListDAO"/>
	</bean>
	
</beans>
