<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- 월별 투입 MAP -->
	
	<bean id="deployMapDAO" class="com.sqisoft.iqs.humanResource.deployMap.dao.impl.DeployMapDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="deployMapService" class="com.sqisoft.iqs.humanResource.deployMap.service.impl.DeployMapServiceImpl">
		<property name="deployMapDAO" ref="deployMapDAO"/>
	</bean>
	
</beans>
