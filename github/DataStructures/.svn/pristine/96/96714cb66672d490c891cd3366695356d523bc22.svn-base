h1{
	margin-top:1px;
}
.contentsTitle {
	border-bottom : 1px solid grey;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

table th {
	background-color : rgb(232, 232, 232);
	border : 1px solid grey;
}

table td {
	border : 1px solid grey;
}
a:visited {
    color: blue;
}