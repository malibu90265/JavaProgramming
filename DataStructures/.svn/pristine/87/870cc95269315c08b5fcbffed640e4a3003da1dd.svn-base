<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="basicInfoController" class="com.sqisoft.iqs.admin.basicinfo.controller.BasicInfoController">
		<property name="basicInfoService" ref="basicInfoService"/>
		<property name="projectService" ref="projectService"/>
	</bean>
	
	<bean id="menuController" class="com.sqisoft.iqs.admin.menu.controller.MenuController">
		<property name="menuService" ref="menuService"/>
	</bean>
	
	<bean id="sessionController" class="com.sqisoft.iqs.admin.menu.controller.SessionController">
	</bean>
	
</beans>
