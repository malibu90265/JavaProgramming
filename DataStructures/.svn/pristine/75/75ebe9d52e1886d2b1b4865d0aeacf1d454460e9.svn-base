package com.sqisoft.iqs.hr.vo;

public class PjNeedsVO {
	/*
	pj_id
	, pn_num
	, pn_grade
	, pn_skill
	, pn_day_from
	, pn_day_to
	, pn_check
	, division
	, area*/
	private String pj_id;
	private int pn_num;
	private String pn_grade;
	private String pn_skill;
	private String pn_day_from;
	private String pn_day_to;
	private String pn_etc;
	private String pn_check;
	private String division;
	private String area;
	
	public PjNeedsVO() {
		super();
	}

	public PjNeedsVO(String pj_id, int pn_num, String pn_grade, String pn_skill,
			String pn_day_from, String pn_day_to, String pn_etc, String pn_check) {
		super();
		this.pj_id = pj_id;
		this.pn_num = pn_num;
		this.pn_grade = pn_grade;
		this.pn_skill = pn_skill;
		this.pn_day_from = pn_day_from;
		this.pn_day_to = pn_day_to;
		this.pn_etc = pn_etc;
		this.pn_check = pn_check;
	}

	
	
	public String getArea() {
		return area;
	}

	public void setArea(String area) {
		this.area = area;
	}

	public String getDivision() {
		return division;
	}

	public void setDivision(String division) {
		this.division = division;
	}

	public String getPj_id() {
		return pj_id;
	}

	public void setPj_id(String pj_id) {
		this.pj_id = pj_id;
	}

	public int getPn_num() {
		return pn_num;
	}

	public void setPn_num(int pn_num) {
		this.pn_num = pn_num;
	}

	public String getPn_grade() {
		return pn_grade;
	}

	public void setPn_grade(String pn_grade) {
		this.pn_grade = pn_grade;
	}

	public String getPn_skill() {
		return pn_skill;
	}

	public void setPn_skill(String pn_skill) {
		this.pn_skill = pn_skill;
	}

	public String getPn_day_from() {
		return pn_day_from;
	}

	public void setPn_day_from(String pn_day_from) {
		this.pn_day_from = pn_day_from;
	}

	public String getPn_day_to() {
		return pn_day_to;
	}

	public void setPn_day_to(String pn_day_to) {
		this.pn_day_to = pn_day_to;
	}

	public String getPn_etc() {
		return pn_etc;
	}

	public void setPn_etc(String pn_etc) {
		this.pn_etc = pn_etc;
	}

	public String getPn_check() {
		return pn_check;
	}

	public void setPn_check(String pn_check) {
		this.pn_check = pn_check;
	}

	@Override
	public String toString() {
		return "PjNeedsVO [pj_id=" + pj_id + ", pn_num=" + pn_num
				+ ", pn_grade=" + pn_grade + ", pn_skill=" + pn_skill
				+ ", pn_day_from=" + pn_day_from + ", pn_day_to=" + pn_day_to
				+ ", pn_etc=" + pn_etc + ", pn_check=" + pn_check + "]";
	}
	
	


	
	
}
