<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- hr.main -->
	<bean id="hrDao" class="com.sqisoft.iqs.hr.main.dao.impl.HrDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="hrService" class="com.sqisoft.iqs.hr.main.service.impl.HrServiceImpl">
		<property name="hrDao" ref="hrDao"/>
		<property name="outSalHistoryDAO" ref="outSalHistoryDAO"/>
	</bean>

	<!-- hr.emp -->
	<bean id="empScheduleDAO" class="com.sqisoft.iqs.hr.emp.dao.impl.EmpScheduleDaoImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="empScheduleService" class="com.sqisoft.iqs.hr.emp.service.impl.EmpScheduleServiceImpl">
		<property name="empScheduleDao" ref="empScheduleDAO"/>
	</bean>
	
	<!-- hr.manplace -->
	<bean id="manPlaceDAO" class="com.sqisoft.iqs.hr.manplace.dao.impl.ManPlaceDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="manPlaceService" class="com.sqisoft.iqs.hr.manplace.service.impl.ManPlaceServiceImpl">
		<property name="manPlaceDao" ref="manPlaceDAO"/>
	</bean>

	<!-- hr.outsal -->
	<bean id="outSalHistoryDAO" class="com.sqisoft.iqs.hr.outsal.dao.impl.OutSalHistoryDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="outSalHistoryService" class="com.sqisoft.iqs.hr.outsal.service.impl.OutSalHistoryServiceImpl">
		<property name="outSalHistoryDAO" ref="outSalHistoryDAO"/>
	</bean>
	
	<!-- hr.pj.needs -->
	<bean id="pjNeedsDAO" class="com.sqisoft.iqs.hr.pj.needs.dao.impl.PjNeedsDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="pjnService" class="com.sqisoft.iqs.hr.pj.needs.service.impl.PjNeedsServiceImpl">
		<property name="pjnDAO" ref="pjNeedsDAO"/>
	</bean>
	
	<!-- hr.pj.posting -->
	<bean id="pjPostingDAO" class="com.sqisoft.iqs.hr.pj.posting.dao.impl.PjpostingDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"/>
	</bean>
	
	<bean id="pjPostingService" class="com.sqisoft.iqs.hr.pj.posting.service.impl.PjpostingServiceImpl">
		<property name="pjpostingDao" ref="pjPostingDAO"/>
		<property name="t_dao" ref="teamDAO"/>
	</bean>
	
</beans>
