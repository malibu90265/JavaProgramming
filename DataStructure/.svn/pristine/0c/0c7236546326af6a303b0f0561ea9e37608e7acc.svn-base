<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="deployRequirementsDAO" class="com.sqisoft.iqs.humanResource.deployRequirements.dao.impl.DeployRequirementsDAOImpl">
		<property name="sqlSessionTemplate" ref="sqlSession"></property>
	</bean>
	
	<bean id="deployRequirementsService" class="com.sqisoft.iqs.humanResource.deployRequirements.service.impl.DeployRequirementsServiceImpl">
		<property name="deployRequirementsDAO" ref="deployRequirementsDAO"/>
	</bean>
	
</beans>
